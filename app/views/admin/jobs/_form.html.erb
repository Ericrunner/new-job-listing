<%= simple_form_for [:admin,@job] do |f|%>
  <%= f.input :title, label: "职位名称", error:"职位名称不能为空！"%>
  <%= f.input :description,label: "职位要求"%>
  <%= f.input :wage_upper_bound,label:"薪资上限"%>
  <%= f.input :wage_down_bound,label:"薪资下限"%>
  <%= f.input :contact_email,label:"联系方式"%>
  <%= f.input :is_hidden, as: :boolean,checked_value: true,unchecked_value: false %>
  <%= f.submit '提交'%>
<% end%>
